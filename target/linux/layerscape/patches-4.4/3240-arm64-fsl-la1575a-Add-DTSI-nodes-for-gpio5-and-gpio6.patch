From 315e3a8afdf5bd892c212cf264b844d3a2e030d8 Mon Sep 17 00:00:00 2001
From: Abhimanyu Saini <abhimanyu.saini@nxp.com>
Date: Thu, 13 Apr 2017 12:12:33 +0530
Subject: [PATCH 13/26] arm64: fsl: la1575a: Add DTSI nodes for gpio5 and gpio6

Signed-off-by: Abhimanyu Saini <abhimanyu.saini@nxp.com>
Signed-off-by: Rajesh Bhagat <rajesh.bhagat@nxp.com>
---
 arch/arm64/boot/dts/freescale/fsl-la1575a.dtsi | 22 ++++++++++++++++++++++
 1 file changed, 22 insertions(+)

diff --git a/arch/arm64/boot/dts/freescale/fsl-la1575a.dtsi b/arch/arm64/boot/dts/freescale/fsl-la1575a.dtsi
index aa4854d..8098faf 100644
--- a/arch/arm64/boot/dts/freescale/fsl-la1575a.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-la1575a.dtsi
@@ -216,6 +216,28 @@
 		#interrupt-cells = <2>;
 	};
 
+	gpio5: gpio@8B75000  {
+		compatible = "fsl,qoriq-gpio";
+		reg = <0x0 0x8B75000  0x0 0x10000>;
+		interrupts = <0 249 0x4>; /* Level high type */
+		gpio-controller;
+		little-endian;
+		#gpio-cells = <2>;
+		interrupt-controller;
+		#interrupt-cells = <2>;
+	};
+
+	gpio6: gpio@8B76000  {
+		compatible = "fsl,qoriq-gpio";
+		reg = <0x0 0x8B76000  0x0 0x10000>;
+		interrupts = <0 250 0x4>; /* Level high type */
+		gpio-controller;
+		little-endian;
+		#gpio-cells = <2>;
+		interrupt-controller;
+		#interrupt-cells = <2>;
+	};
+
 	ftm0: ftm0@2800000 {
 		compatible = "fsl,ftm-alarm";
 		reg = <0x0 0x2800000 0x0 0x10000>;
-- 
2.7.4

